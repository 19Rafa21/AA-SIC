<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="backend.User" table="`User`" lazy="false" discriminator-value="User">
		<id name="id" column="Id" type="string">
			<generator class="assigned">
			</generator>
		</id>
		<discriminator column="Discriminator" type="string"/>
		<property name="username" column="Username" type="string" length="255" not-null="false" lazy="false"/>
		<property name="password" column="Password" type="string" length="255" not-null="false" lazy="false"/>
		<property name="email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<subclass name="backend.Client" discriminator-value="Client" lazy="false">
		</subclass>
		<subclass name="backend.Owner" discriminator-value="Owner" lazy="false">
		</subclass>
	</class>
</hibernate-mapping>
