<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="backend.Reply" table="Reply" lazy="false">
		<id name="id" column="Id" type="string">
			<generator class="assigned">
			</generator>
		</id>
		<property name="text" column="Text" type="string" length="255" not-null="false" lazy="false"/>
		<property name="review" column="Review" type="string" length="255" not-null="false" lazy="false"/>
		<property name="author" column="Author" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
